Prog. Program ::= [ Decl ] ;
separator Decl ";" ;

Def. Decl ::= Ident ":" Exp "=" [ Branch ]  ;
separator Branch "|" ;

Br. Branch ::= [ Exp ] "->" Exp ;
separator Exp " " ;

Pi.  Exp ::= "(" Ident ":" Exp ")" "->" Exp ;
Arr. Exp ::= Exp1 "->" Exp;
Sigma. Exp ::= "(" Ident ":" Exp ")" "*" Exp ;
Type. Exp2 ::= "Type";

Lam. Exp ::= "\\" Abstr;
App. Exp1 ::= Ident [ Exp ];

Abstr. Abstr ::= Ident "." Exp;

coercions Exp 3;